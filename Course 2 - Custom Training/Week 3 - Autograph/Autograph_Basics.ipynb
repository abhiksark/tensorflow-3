        "@tf.function\n",
        "def fizzbuzz(max_num):\n",
        "  counter = 0\n",
        "  for num in range(max_num):\n",
        "    if num % 3 == 0 and num % 5 == 0:\n",
        "      print('FizzBuzz')\n",
        "    elif num % 3 == 0:\n",
        "      print('Fizz')\n",
        "    elif num % 5 == 0:\n",
        "      print('Buzz')\n",
        "    else:\n",
        "      print(num)\n",
        "    counter += 1\n",
        "  return counter\n",
